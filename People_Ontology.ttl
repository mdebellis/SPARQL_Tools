@prefix : <http://michaeldebellis.com/people/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@base <http://michaeldebellis.com/people/> .

<http://michaeldebellis.com/people> rdf:type owl:Ontology ;
                                     dct:creator """Michael DeBellis
Orcid ID: 0000-0002-8824-9577""" ;
                                     dct:license <https://creativecommons.org/licenses/by/4.0/legalcode> ;
                                     skos:definition "A simple example ontology of People, Organizations, Places, and Animals to illustrate basic Semantic Web concepts from RDF, RDFS, OWL, SPARQL and SHACL" .

#################################################################
#    Annotation properties
#################################################################

###  http://michaeldebellis.com/people/wikidata
:wikidata rdf:type owl:AnnotationProperty ;
          rdfs:range xsd:anyURI .


###  http://purl.org/dc/terms/abstract
dct:abstract rdf:type owl:AnnotationProperty ;
             rdfs:label "abstract" ;
             skos:definition "A summary of the resource." .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty ;
            rdfs:comment "Recommended practice is to identify the creator with a URI. If this is not possible or feasible, a literal value that identifies the creator may be provided." ;
            rdfs:label "creator" ;
            skos:definition "An entity responsible for making the resource." .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/subject
dct:subject rdf:type owl:AnnotationProperty ;
            rdfs:comment "Recommended practice is to refer to the subject with a URI. If this is not possible or feasible, a literal value that identifies the subject may be provided. Both should preferably refer to a subject in a controlled vocabulary." ;
            rdfs:label "subject" ;
            skos:definition "A topic of the resource." .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty ;
          rdfs:label "title" ;
          skos:definition "A name given to the resource." .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty ;
              rdfs:isDefinedBy <https://www.w3.org/2012/09/odrl/semantic/draft/doco/http___www.w3.org_2004_02_skos_core.html> ;
              rdfs:label "alternative label"@en ;
              skos:definition "An alternative lexical label for a resource." ;
              skos:example "Acronyms, abbreviations, spelling variants, and irregular plural/singular forms may be included among the alternative labels for a concept. Mis-spelled terms are normally included as hidden labels (see skos:hiddenLabel)." ;
              rdfs:subPropertyOf rdfs:label .


###  http://www.w3.org/2004/02/skos/core#changeNote
skos:changeNote rdf:type owl:AnnotationProperty ;
                rdfs:label "change note"@en ;
                skos:definition "A note about a modification to a concept." ;
                rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty ;
                rdfs:label "definition"@en ;
                skos:definition "A statement or formal explanation of the meaning of a concept." ;
                rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty ;
             rdfs:label "example"@en ;
             skos:definition "An example of the use of a concept." ;
             rdfs:subPropertyOf skos:note .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty ;
          rdfs:isDefinedBy <https://www.w3.org/2012/09/odrl/semantic/draft/doco/http___www.w3.org_2004_02_skos_core.html> ;
          rdfs:label "note"@en ;
          skos:definition "A general note, for any purpose."@en .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty ;
               rdfs:comment "skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties.	The range of skos:prefLabel is the class of RDF plain literals. A resource has no more than one value of skos:prefLabel per language tag." ;
               rdfs:isDefinedBy <https://www.w3.org/2012/09/odrl/semantic/draft/doco/http___www.w3.org_2004_02_skos_core.html> ;
               rdfs:label "preferred label"@en ;
               skos:definition "The preferred lexical label for a resource, in a given language." ;
               rdfs:subPropertyOf rdfs:label .


#################################################################
#    Object Properties
#################################################################

###  http://michaeldebellis.com/people/contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :is_contained_in ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :Place ;
          rdfs:range :Place ;
          rdfs:label "contains" .


###  http://michaeldebellis.com/people/contains_Direct
:contains_Direct rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :contains ;
                 owl:inverseOf :is_Contained_In_Direct ;
                 rdfs:label "contains direct" .


###  http://michaeldebellis.com/people/eats
:eats rdf:type owl:ObjectProperty ;
      rdfs:domain :Animal ;
      rdfs:range :Living_Thing ;
      rdfs:label "eats" .


###  http://michaeldebellis.com/people/has_Address
:has_Address rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_Location ;
             owl:inverseOf :is_Address_For ;
             rdfs:domain :Agent ;
             rdfs:range :Address ;
             rdfs:label "has address" .


###  http://michaeldebellis.com/people/has_Aunt
:has_Aunt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :has_Family_Relationship ;
          owl:inverseOf :is_Aunt_Of ;
          rdfs:label "has aunt" .


###  http://michaeldebellis.com/people/has_Brother
:has_Brother rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_Sibling ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Boy
                                        :Man
                                      )
                        ] ;
             rdfs:label "has brother" .


###  http://michaeldebellis.com/people/has_Child
:has_Child rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :has_Family_Relationship ;
           owl:inverseOf :has_Parent ;
           rdfs:label "has child" .


###  http://michaeldebellis.com/people/has_Colleague
:has_Colleague rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :has_Social_Relationship ;
               rdf:type owl:SymmetricProperty ;
               rdfs:label "has colleague" .


###  http://michaeldebellis.com/people/has_Daughter
:has_Daughter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :has_Child ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Girl
                                         :Woman
                                       )
                         ] ;
              rdfs:label "has daughter" .


###  http://michaeldebellis.com/people/has_Family_Relationship
:has_Family_Relationship rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :has_Social_Relationship ;
                         rdfs:label "has family relationship" .


###  http://michaeldebellis.com/people/has_Father
:has_Father rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Parent ;
            rdfs:range :Man ;
            :wikidata <https://www.wikidata.org/wiki/Q7565> ;
            rdfs:label "has father" .


###  http://michaeldebellis.com/people/has_Friend
:has_Friend rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Social_Relationship ;
            rdf:type owl:SymmetricProperty ;
            rdfs:label "has friend" .


###  http://michaeldebellis.com/people/has_Gender
:has_Gender rdf:type owl:ObjectProperty ;
            rdfs:domain :Animal ;
            rdfs:range :Gender ;
            rdfs:label "has gender" .


###  http://michaeldebellis.com/people/has_Husband
:has_Husband rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_Spouse ;
             rdfs:range :Man ;
             rdfs:label "has husband" .


###  http://michaeldebellis.com/people/has_Location
:has_Location rdf:type owl:ObjectProperty ;
              rdfs:range :Place ;
              rdfs:label "has location" .


###  http://michaeldebellis.com/people/has_Member
:has_Member rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Part ;
            owl:inverseOf :is_Member_Of ;
            rdfs:domain :Organization ;
            rdfs:range :Agent ;
            rdfs:label "has member" .


###  http://michaeldebellis.com/people/has_Mother
:has_Mother rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Parent ;
            rdfs:range :Woman ;
            :wikidata <https://www.wikidata.org/wiki/Q7560> ;
            rdfs:label "has mother" .


###  http://michaeldebellis.com/people/has_Parent
:has_Parent rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Family_Relationship ;
            rdfs:label "has parent" .


###  http://michaeldebellis.com/people/has_Part
:has_Part rdf:type owl:ObjectProperty ;
          owl:inverseOf :is_Part_Of ;
          rdfs:label "has part" .


###  http://michaeldebellis.com/people/has_Pet
:has_Pet rdf:type owl:ObjectProperty ;
         owl:inverseOf :is_Pet_Of ;
         rdfs:label "has pet" .


###  http://michaeldebellis.com/people/has_Sibling
:has_Sibling rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_Family_Relationship ;
             rdf:type owl:SymmetricProperty ;
             rdfs:label "has sibling" .


###  http://michaeldebellis.com/people/has_Sister
:has_Sister rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Sibling ;
            rdfs:range [ rdf:type owl:Restriction ;
                         owl:onProperty :has_Gender ;
                         owl:hasValue :Female
                       ] ;
            rdfs:label "has sister" .


###  http://michaeldebellis.com/people/has_Social_Relationship
:has_Social_Relationship rdf:type owl:ObjectProperty ,
                                  owl:SymmetricProperty ;
                         rdfs:domain :Person ;
                         rdfs:range :Person ;
                         rdfs:label "has social relationship" .


###  http://michaeldebellis.com/people/has_Son
:has_Son rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :has_Child ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Boy
                                    :Man
                                  )
                    ] ;
         rdfs:label "has son" .


###  http://michaeldebellis.com/people/has_Spouse
:has_Spouse rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Family_Relationship ;
            rdf:type owl:FunctionalProperty ,
                     owl:SymmetricProperty ;
            rdfs:domain :Adult ;
            rdfs:range :Adult ;
            rdfs:label "has spouse" .


###  http://michaeldebellis.com/people/has_State
:has_State rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :has_Location ;
           rdfs:range :State ;
           rdfs:label "has state" .


###  http://michaeldebellis.com/people/has_Subsidiary
:has_Subsidiary rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :has_Part ;
                owl:inverseOf :is_subsidiary_of ;
                rdfs:domain :Organization ;
                rdfs:range :Orgonization_Component ;
                rdfs:label "has subsidiary" .


###  http://michaeldebellis.com/people/has_Uncle
:has_Uncle rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :has_Family_Relationship ;
           owl:inverseOf :is_Uncle_Of ;
           rdfs:label "has uncle" .


###  http://michaeldebellis.com/people/has_Wife
:has_Wife rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :has_Spouse ;
          rdfs:range :Woman ;
          rdfs:label "has wife" .


###  http://michaeldebellis.com/people/has_city
:has_city rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :has_Location ;
          rdfs:range :City ;
          rdfs:label "has city" .


###  http://michaeldebellis.com/people/has_employer
:has_employer rdf:type owl:ObjectProperty ;
              owl:inverseOf :is_Employer_Of ;
              rdfs:domain :Employee ;
              rdfs:range :Agent ;
              rdfs:label "has employer" .


###  http://michaeldebellis.com/people/is_Address_For
:is_Address_For rdf:type owl:ObjectProperty ;
                rdfs:label "is address for" .


###  http://michaeldebellis.com/people/is_Aunt_Of
:is_Aunt_Of rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :has_Family_Relationship ;
            rdfs:label "is aunt of" .


###  http://michaeldebellis.com/people/is_Contained_In_Direct
:is_Contained_In_Direct rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :is_contained_in ;
                        rdfs:label "is contained in direct" .


###  http://michaeldebellis.com/people/is_Employer_Of
:is_Employer_Of rdf:type owl:ObjectProperty ;
                rdfs:label "is employer of" .


###  http://michaeldebellis.com/people/is_Member_Of
:is_Member_Of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :is_Part_Of ;
              rdfs:label "is member of" .


###  http://michaeldebellis.com/people/is_Part_Of
:is_Part_Of rdf:type owl:ObjectProperty ;
            rdfs:label "is part of" .


###  http://michaeldebellis.com/people/is_Pet_Of
:is_Pet_Of rdf:type owl:ObjectProperty ;
           rdfs:domain :Pet ;
           rdfs:range :Human ;
           rdfs:label "is pet of" .


###  http://michaeldebellis.com/people/is_Uncle_Of
:is_Uncle_Of rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_Family_Relationship ;
             rdfs:label "is uncle of" .


###  http://michaeldebellis.com/people/is_contained_in
:is_contained_in rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:label "is contained in" .


###  http://michaeldebellis.com/people/is_subsidiary_of
:is_subsidiary_of rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :is_Part_Of ;
                  rdfs:label "is subsidiary of" .


#################################################################
#    Data properties
#################################################################

###  http://michaeldebellis.com/people/birth_date
:birth_date rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:range xsd:dateTime ;
            rdfs:label "birth date" .


###  http://michaeldebellis.com/people/email
:email rdf:type owl:DatatypeProperty ;
       rdfs:label "email" .


###  http://michaeldebellis.com/people/employee_id
:employee_id rdf:type owl:DatatypeProperty ;
             rdfs:domain :Employee ;
             rdfs:label "employee id" .


###  http://michaeldebellis.com/people/has_age
:has_age rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Person ;
         rdfs:range xsd:integer ;
         rdfs:label "age" ;
         skos:example 12 ,
                      32 ;
         skos:prefLabel "has age" .


###  http://michaeldebellis.com/people/hired_on_date
:hired_on_date rdf:type owl:DatatypeProperty ;
               rdfs:domain :Employee ;
               rdfs:range xsd:dateTime ;
               rdfs:label "hired on date" .


###  http://michaeldebellis.com/people/nobel_prize_winner
:nobel_prize_winner rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Person ;
                    rdfs:range xsd:boolean ;
                    rdfs:label "nobel prize winner" .


###  http://michaeldebellis.com/people/number_of_social_relations
:number_of_social_relations rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain :Person ;
                            rdfs:range xsd:integer ;
                            rdfs:label "number of social relations" .


###  http://michaeldebellis.com/people/phone
:phone rdf:type owl:DatatypeProperty ;
       rdfs:label "phone" .


###  http://michaeldebellis.com/people/street
:street rdf:type owl:DatatypeProperty ;
        rdfs:domain :Address ;
        rdfs:label "street" .


###  http://michaeldebellis.com/people/unit
:unit rdf:type owl:DatatypeProperty ;
      rdfs:domain :Address ;
      rdfs:label "unit" ;
      skos:altLabel "apartment" ,
                    "suite" .


###  http://michaeldebellis.com/people/zip
:zip rdf:type owl:DatatypeProperty ;
     rdfs:domain :Address ;
     rdfs:label "zip" .


#################################################################
#    Classes
#################################################################

###  http://michaeldebellis.com/people/Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :Place ;
         rdfs:label "Address" .


###  http://michaeldebellis.com/people/Adult
:Adult rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Person
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :has_age ;
                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                         owl:onDatatype xsd:integer ;
                                                                         owl:withRestrictions ( [ xsd:minInclusive 18
                                                                                                ]
                                                                                              )
                                                                       ]
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:label "Adult" .


###  http://michaeldebellis.com/people/Agent
:Agent rdf:type owl:Class ;
       rdfs:label "Agent" .


###  http://michaeldebellis.com/people/Animal
:Animal rdf:type owl:Class ;
        rdfs:subClassOf :Living_Thing ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :eats ;
                          owl:someValuesFrom :Living_Thing
                        ] ;
        owl:disjointWith :Plant ;
        rdfs:label "Animal" .


###  http://michaeldebellis.com/people/Bird
:Bird rdf:type owl:Class ;
      rdfs:subClassOf :Vertebrate ;
      rdfs:label "Bird" .


###  http://michaeldebellis.com/people/Boy
:Boy rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :Child
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :has_Gender ;
                                                  owl:hasValue :Male
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:subClassOf :Child ;
     owl:disjointWith :Girl ;
     rdfs:label "Boy" .


###  http://michaeldebellis.com/people/Cat
:Cat rdf:type owl:Class ;
     rdfs:subClassOf :Mammal ;
     rdfs:label "Cat" .


###  http://michaeldebellis.com/people/Child
:Child rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Person
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :has_age ;
                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                         owl:onDatatype xsd:integer ;
                                                                         owl:withRestrictions ( [ xsd:maxExclusive 18
                                                                                                ]
                                                                                              )
                                                                       ]
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Person ;
       rdfs:label "Child" .


###  http://michaeldebellis.com/people/City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ;
      rdfs:label "City" .


###  http://michaeldebellis.com/people/Continent
:Continent rdf:type owl:Class ;
           rdfs:subClassOf :Place ;
           rdfs:label "Continent" .


###  http://michaeldebellis.com/people/Corporation
:Corporation rdf:type owl:Class ;
             rdfs:subClassOf :Organization ;
             rdfs:label "Corporation" .


###  http://michaeldebellis.com/people/Cow
:Cow rdf:type owl:Class ;
     rdfs:subClassOf :Mammal ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :eats ;
                       owl:allValuesFrom :Grass
                     ] ;
     rdfs:label "Cow" .


###  http://michaeldebellis.com/people/Division
:Division rdf:type owl:Class ;
          rdfs:subClassOf :Orgonization_Component ;
          rdfs:label "Division" .


###  http://michaeldebellis.com/people/Dog
:Dog rdf:type owl:Class ;
     rdfs:subClassOf :Mammal ;
     rdfs:label "Dog" .


###  http://michaeldebellis.com/people/Einsteins_Wife
:Einsteins_Wife rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Woman
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :has_Husband ;
                                                             owl:hasValue :Albert_Einstein
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:label "Einstein's Wife" .


###  http://michaeldebellis.com/people/Employee
:Employee rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :has_employer ;
                                                       owl:someValuesFrom :Agent
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Person ;
          rdfs:label "Employee" .


###  http://michaeldebellis.com/people/Fish
:Fish rdf:type owl:Class ;
      rdfs:subClassOf :Vertebrate ;
      rdfs:label "Fish" .


###  http://michaeldebellis.com/people/Flower
:Flower rdf:type owl:Class ;
        rdfs:subClassOf :Plant ;
        owl:disjointWith :Tree ;
        rdfs:label "Flower" .


###  http://michaeldebellis.com/people/Gender
:Gender rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Female
                                          :Male
                                          :Non_Binary
                                        )
                            ] ;
        rdfs:label "Gender" .


###  http://michaeldebellis.com/people/Girl
:Girl rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Child
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_Gender ;
                                                   owl:hasValue :Female
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Child ;
      rdfs:label "Girl" .


###  http://michaeldebellis.com/people/Gold_Fish
:Gold_Fish rdf:type owl:Class ;
           rdfs:subClassOf :Fish ;
           rdfs:label "Gold Fish" .


###  http://michaeldebellis.com/people/Grass
:Grass rdf:type owl:Class ;
       rdfs:subClassOf :Plant ;
       rdfs:label "Grass" .


###  http://michaeldebellis.com/people/Haddock
:Haddock rdf:type owl:Class ;
         rdfs:subClassOf :Fish ;
         rdfs:label "Haddock" .


###  http://michaeldebellis.com/people/Halibut
:Halibut rdf:type owl:Class ;
         rdfs:subClassOf :Fish ;
         rdfs:label "Halibut" .


###  http://michaeldebellis.com/people/Hedgehog
:Hedgehog rdf:type owl:Class ;
          rdfs:subClassOf :Mammal ;
          rdfs:label "Hedgehog" .


###  http://michaeldebellis.com/people/Herbivore
:Herbivore rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Animal
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :eats ;
                                                        owl:allValuesFrom :Plant
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Animal ;
           rdfs:label "Herbivore" .


###  http://michaeldebellis.com/people/Hermit
:Hermit rdf:type owl:Class ;
